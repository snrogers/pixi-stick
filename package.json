{
    "name": "pixi-stick",
    "version": "0.0.2",
    "description": "a touchscreen thumbstick for Pixi.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/snrogers/pixi-stick.git"
    },
    "main": "dist/index.js",
    "scripts": {
        "postversion": "npm run bowerSync && npm run changelog",
        "buildWatch": "npm run concat && npm run umd",
        "postbuildWatch": "npm run clean && npm run copyToTest",
        "build": "npm run buildWatch && npm run uglify",
        "formatAndLint": "npm run format && npm run lint",
        "prebuild": "npm run formatAndLint",
        "pretest": "npm run build",
        "changelog": "node ./bin/changelog.js",
        "bowerSync": "node ./bin/bowerSync.js",
        "clean": "node ./bin/clean.js",
        "copyToTest": "node ./bin/copyToTest.js",
        "watch": "watch \"npm run buildWatch\" src",
        "start": "static test -a 127.0.0.1:80",
        "toc": "markdown-toc -i README.md"
    },
    "dependencies": {
        "node-uuid": "^1.4.7"
    },
    "devDependencies": {
        "autodts": "0.0.6",
        "awesome-typescript-loader": "^2.2.1",
        "casperjs": "yoannmoinet/casperjs#a45c64d",
        "chalk": "^1.1.3",
        "concat-cli": "yoannmoinet/concat-cli",
        "conventional-changelog": "1.1.0",
        "eslint": "^1.1.0",
        "expect.js": "^0.3.1",
        "fs": "0.0.2",
        "gulp": "^3.9.1",
        "gulp-concat": "^2.6.0",
        "gulp-delete-lines": "0.0.7",
        "gulp-typescript": "^2.13.6",
        "jscs": "^2.1.0",
        "markdown-toc": "^0.12.12",
        "mocha": "^2.2.5",
        "node-static": "^0.7.7",
        "phantomjs": "yoannmoinet/phantomjs#nick-2",
        "pixi.js": "^4.0.0",
        "source-map-loader": "^0.1.5",
        "ts-loader": "^0.8.2",
        "typescript": "^1.8.10",
        "uglify-js": "^2.4.24",
        "umd": "^3.0.1",
        "umd-generator": "^1.0.2",
        "watch": "^0.16.0",
        "webpack": "^1.13.1"
    },
    "peerDependencies": {
        "pixi.js": "^4.0.0"
    },
    "author": "",
    "license": "ISC",
    "homepage": "https://bitbucket.org/snrogers/pixi-stick#readme"
}